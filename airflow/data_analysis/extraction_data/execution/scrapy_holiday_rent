
#!/bin/bash

#Atraveo
scrapy crawl atraveo_listHomes -a place='Gran Canaria' -o GranCanaria_atraveo.csv 2>&1 | tee GranCanaria_atraveo.txt
scrapy crawl atraveo_listHomes -a place='Fuerteventura' -o Fuerteventura_atraveo.csv 2>&1 | tee Fuerteventura_atraveo.txt
scrapy crawl atraveo_listHomes -a place='Lanzarote' -o Lanzarote_atraveo.csv 2>&1 | tee Lanzarote_atraveo.txt
scrapy crawl atraveo_listHomes -a place='La Palma' -o LaPalma_atraveo.csv 2>&1 | tee LaPalma_atraveo.txt
scrapy crawl atraveo_listHomes -a place='La Gomera' -o LaGomera_atraveo.csv 2>&1 | tee LaGomera_atraveo.txt
scrapy crawl atraveo_listHomes -a place='El Hierro' -o ElHierro_atraveo.csv 2>&1 | tee ElHierro_atraveo.txt
#scrapy crawl atraveo_listHomes -a place='Tenerife' -o Tenerife_atraveo.csv 2>&1 | tee Tenerife_atraveo.txt

